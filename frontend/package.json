{
    "name": "audio-hq-next",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
        "analyze": "cross-env ANALYZE=true next build",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "gql:update": "graphql-codegen --config codegen.yml -r dotenv/config",
        "hasura:start": "docker compose up -d --project-directory ..",
        "hasura:stop": "docker compose down --project-directory ..",
        "hasura:apply": "cd ../hasura && hasura migrate apply --up all --all-databases && hasura metadata apply && cd -",
        "hasura:update-config": "cd ../hasura && hasura metadata export && cd -",
        "hasura:full-reset": "docker compose down -v  --project-directory .."
    },
    "cacheDirectories": [
        "node_modules/",
        ".next/cache/"
    ],
